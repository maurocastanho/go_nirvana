{
    "options": [{"Name": "filename_field", "Value": "ID"},
        {"Name": "name_field", "Value": "Título Original"},
        {"Name": "categ_field1", "Value": "Genero 1"},
        {"Name": "categ_field2", "Value": "Genero 2"}
    ],
    "xls_output": {
        "filename": "publicar_box.xls",
        "sheet": "Formulário",
        "columns": [
            {"Name": "ASSET_ID", "function": "assetid", "suffix_number": 1, "prefix": "Provider"},
            {"Name": "ORIGINAL TITLE", "field": "Título Original", "function": "field_raw"},
            {"Name": "TÍTULO EM PORTUGUÊS", "field": "Título em Português", "function": "field_raw"},
            {"Name": "NOME DO EPISÓDIO", "field": "Título em Português do Episódio", "function": "field_raw"},
            {"Name": "TEMPORADA", "field": "Temporada", "function": "field_raw", "format": "0"},
            {"Name": "EPISÓDIO", "field": "Número do Episódio", "function": "field_raw", "format": "0"},
            {"Name": "PRODUTO", "field": "Produto", "function": "field_raw"},
            {"Name": "PROVEDOR", "field": "Provider", "function": "field_raw"},
            {"Name": "JANELA DE REPASSE", "field": "Billing ID", "function": "janela_repasse"},
            {"Name": "ESTÚDIO", "field": "Estúdio", "function": "field_raw"},
            {"Name": "CANAL", "field": "Canal", "function": "field_raw"},
            {"Name": "CATEGORIA", "field": "Categoria", "function": "field_raw"},
            {"Name": "GÊNERO 1", "field": "Genero 1", "function": "field_raw"},
            {"Name": "GÊNERO 2", "field": "Genero 2", "function": "field_raw"},
            {"Name": "ANO DE PRODUÇÃO", "field": "Ano", "function": "field_raw", "format": "0"},
            {"Name": "BOX OFFICE", "field": "Box Office", "function": "field_raw", "format": "#,##0.00"},
            {"Name": "RANKING", "field": "Ranking", "function": "field_raw", "format": "#,##0.0"},
            {"Name": "LÍNGUA ORIGINAL", "field": "Língua Original", "function": "field_raw"},
            {"Name": "RATING", "field": "Classificação Etária", "function": "field_raw", "format": "0"},
            {"Name": "ELENCO", "field": "Elenco", "function": "field_raw"},
            {"Name": "DIRETOR", "field": "Diretor", "function": "field_raw"},
            {"Name": "PAÍS DE ORIGEM", "field": "País de Origem", "function": "field_raw"},
            {"Name": "SINOPSE", "field": "Sinopse EPG", "function": "field_raw"},
            {"Name": "DURAÇÃO", "field": "Duração", "function": "field", "format": "HH:MM:SS", "type": "time_s"},
            {"Name": "LICENÇA INÍCIO", "field": "Data Início", "function": "field_date", "format": "DD/MM/YYYY"},
            {"Name": "LICENÇA FIM", "field": "Data Fim", "function": "field_date", "format": "DD/MM/YYYY"},
            {"Name": "TECNOLOGIA", "field": "Formato", "function": "field_raw"},
            {"Name": "AUDIO", "field": "Audio", "function": "field_raw"},
            {"Name": "VERSAO", "field": "Versao", "function": "field_raw"},
            {"Name": "COBRANÇA", "field": "Cobrança", "function": "field", "format": "\"R$\" #,##0.00", "type": "money"},
            {"Name": "DOWNLOAD TO GO", "field": "DOWNLOAD TO GO", "function": "field_raw"},
            {"Name": "DIAS DE DOWNLOAD", "field": "DIAS DE DOWNLOAD", "function": "field_raw", "format": "0"},
            {"Name": "PASTA FTP", "field": "PASTA FTP", "function": "field_raw"},
            {"Name": "BILLING ID", "field": "Billing ID", "function": "field_raw"}
        ]
    },
    "elements": [
        {
            "Name": "assets",
            "elements": [
                {
                    "attrs": [
                        {"Name": "id", "function": "uuid"},
                        {
                            "Name": "title",
                            "no_array": "",
                            "elements": [{"attrs": [{"Name": "por", "function": "field", "field": "Título"}]}]
                        },
                        {
                            "Name": "synopsis",
                            "no_array": "",
                            "elements": [{"attrs": [{"Name": "por", "function": "field", "field": "Sinopse EPG"}]}]
                        },
                        {"Name": "morality_level", "function": "field", "field": "Classificação Etária", "type": "int"},
                        {"Name": "adult", "function": "condition", "condition": "Classificação_Etária == 18", "if_true": "true", "if_false": "false", "type": "boolean"},
                        {"Name": "duration", "function": "field", "field": "Duração", "type": "timestamp"},
                        {"Name": "series_id", "function": "uuid", "filter": "Número do Episódio != ''"},
                        {"Name": "season_id", "function": "uuid", "filter": "Número do Episódio != ''"},
                        {"Name": "season_episode_number", "function": "field", "field": "Número do Episódio", "filter": "Número do Episódio != ''"},
                        {
                            "Name": "genres",
                            "elem_val": "",
                            "elements": [{"attrs": [{"Name": "genres", "function": "field", "field": "Genero 1"}]}]
                        },
                        {"Name": "available_from", "function": "timestamp", "field": "Data Início", "type": "int"},
                        {"Name": "available_to", "function": "timestamp", "field": "Data Fim", "type": "int"},
                        {"Name": "images", "elements": []},
                        {
                            "Name": "metadata",
                            "no_array": "",

                            "elements": [
                                {
                                    "attrs": [
                                        {
                                            "Name": "rights",
                                            "function": "fixed",
                                            "Value": "",
                                            "elements": [

                                            ]
                                        },
                                        {
                                            "Name": "country",
                                            "function": "field",
                                            "field": "País de Origem"
                                        },
                                        {
                                            "Name": "release_year",
                                            "function": "field",
                                            "field": "Ano"
                                        },
                                        {
                                            "Name": "summary",
                                            "function": "field",
                                            "field": "Sinopse Resumo"
                                        },
                                        {
                                            "Name": "actors",
                                            "function": "split",
                                            "function2": "field",
                                            "field": "Elenco"
                                        },
                                        {
                                            "Name": "directors",
                                            "function": "split",
                                            "function2": "field",
                                            "field": "Diretor"
                                        }
                                    ]
                                }
                        ]},
                        {
                            "Name": "medias",
                            "elements": [
                                {
                                    "attrs": [
                                        {"Name": "id", "function": "uuid"},
                                        {"Name": "type", "function": "fixed", "Value": "MEDIA"},
                                        {"Name": "title", "function": "fixed", "Value": "Trailer"},
                                        {"Name": "location", "function": "field", "field": "Trailer ID"},
                                        {"Name": "technology", "function": "fixed", "Value": "MP4"},
                                        {
                                            "Name": "audio_languages",
                                            "elem_val": "",
                                            "elements": [
                                                {
                                                    "Name": "kkk",
                                                    "only_values": "",
                                                    "attrs": [
                                                        {
                                                            "Name": "audio_languages1",
                                                            "function": "split",
                                                            "function2": "field",
                                                            "field": "Língua Original",
                                                            "atype": "value"
                                                        }
                                                    ]
                                                }
                                            ]

                                        },
                                        {
                                            "Name": "subtitles_languages",
                                            "elem_val": "",
                                            "elements": [
                                                {
                                                    "Name": "kkk",
                                                    "only_values": "",
                                                    "attrs": [
                                                        {
                                                            "Name": "audio_languages1",
                                                            "function": "split",
                                                            "function2": "field",
                                                            "field": "Linguagem Legenda",
                                                            "atype": "value"
                                                        }
                                                    ]
                                                }
                                            ]

                                        },

                                        {"Name": "metadata", "elements": []}
                                    ]
                                }
                            ]
                        }
                    ]
                }
            ]
        }
    ]
}
