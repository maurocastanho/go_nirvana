{
    "options": [
        {"Name": "name_field", "Value": "ID"},
        {"Name": "filename_field", "Value": "ID"},
        {"Name": "owner", "Value": "gvt"},
        {"Name": "doctype", "Value": "ADI"},
        {"Name": "doctype_system", "Value": "ADI.DTD"},
        {"Name": "season_field", "Value": "Season"},
        {"Name": "episode_field", "Value": "Número do Episódio"}
    ],
    "xls_output": {
        "filename": "publicar_vivo.xls",
        "sheet": "Formulário",
        "columns": [
            {"Name": "ASSET_ID", "function": "field", "field": "Asset_ID"},
            {"Name": "ORIGINAL TITLE", "field": "Original_Title", "function": "field_raw"},
            {"Name": "TÍTULO EM PORTUGUÊS", "field": "Title", "function": "field_raw"},
            {"Name": "NOME DO EPISÓDIO", "field": "Título em Português do Episódio", "function": "field_raw"},
            {"Name": "TEMPORADA", "field": "Season", "function": "field_raw", "format": "0"},
            {"Name": "EPISÓDIO", "field": "Número do Episódio", "function": "field_raw", "format": "0"},
            {"Name": "PROVEDOR", "field": "Provider", "function": "field_raw"},
            {"Name": "JANELA DE REPASSE", "field": "Billing_ID", "function": "janela_repasse"},
            {"Name": "CATEGORIA", "field": "Content_Category", "function": "field_raw"},
            {"Name": "GÊNERO 1", "field": "Genre 1", "function": "field_raw"},
            {"Name": "GÊNERO 2", "field": "Genre 1", "function": "field_raw"},
            {"Name": "ANO DE PRODUÇÃO", "field": "Year", "function": "field_raw", "format": "0"},
            {"Name": "RATING", "field": "Rating", "function": "field_raw", "format": "0"},
            {"Name": "ELENCO", "field": "Actors", "function": "field_raw"},
            {"Name": "DIRETOR", "field": "Director", "function": "field_raw"},
            {"Name": "PAÍS DE ORIGEM", "field": "Country_of_Origin", "function": "field_raw"},
            {"Name": "SINOPSE", "field": "Summary_Medium", "function": "field_raw"},
            {"Name": "DURAÇÃO", "field": "Run_Time", "function": "field", "format": "HH:MM:SS", "type": "time_s"},
            {"Name": "AUDIO", "field": "Language_movie", "function": "field_raw"},
            {"Name": "BILLING ID", "field": "Billing_ID", "function": "field_raw"}
        ]
    },
    "elements": [
        {
            "Name": "ADI",
            "attrs": [{"Name": "xmlns", "Value": "http://www.eventis.nl/PRODIS/ADI", "function": "fixed"}],
            "elements": [
                {
                    "Name": "Metadata",
                    "elements": [
                        {
                            "Name": "AMS",
                            "attrs": [
                                {"Name": "Provider", "field": "Provider", "function": "field"},
                                {"Name": "Product", "Value": "MOD", "function": "fixed"},
                                {"Name": "Asset_Name", "maxlength": "42", "field": "Title", "function": "field_suffix", "suffix": "_Package"},
                                {"Name": "Version_Major", "Value": "1", "function": "fixed"},
                                {"Name": "Version_Minor", "Value": "0", "function": "fixed"},
                                {"Name": "Description", "field": "Title", "function": "field"},
                                {"Name": "Creation_Date", "function": "date"},
                                {"Name": "Provider_ID", "field": "Provider_ID", "function": "field"},
                                {"Name": "Asset_ID", "function": "field", "field": "Asset_ID"},
                                {"Name": "Asset_Class", "Value": "package", "function": "fixed"}
                            ]
                        },
                        {"Name": "App_Data", "common_attrs": {"App": "MOD"},"single_attrs": [{"Name": "Metadata_Spec_Version", "Value": "CableLabsVOD1.1", "function": "fixed"}]}
                    ]
                },
                {
                    "Name": "Asset",
                    "elements": [
                        {
                            "Name": "Metadata",
                            "options": {"Name": "section", "Value": "title"},
                            "elements": [
                                {
                                    "Name": "AMS",
                                    "attrs": [
                                        {"Name": "Provider", "field": "Provider", "function": "field"},
                                        {"Name": "Product", "Value": "", "function": "fixed"},
                                        {"Name": "Asset_Name", "maxlength": "42", "field": "Title", "function": "field_suffix", "suffix": "_Title"},
                                        {"Name": "Version_Major", "Value": "1", "function": "fixed"},
                                        {"Name": "Version_Minor", "Value": "0", "function": "fixed"},
                                        {"Name": "Description", "field": "Title", "function": "field"},
                                        {"Name": "Creation_Date", "function": "date"},
                                        {"Name": "Provider_ID", "field": "Provider_ID", "function": "field"},
                                        {"Name": "Asset_ID", "function": "assetid", "suffix_number": 2, "prefix": "Provider"},
                                        {"Name": "Asset_Class", "Value": "title", "function": "fixed"}
                                    ]
                                },
                                {
                                    "Name": "App_Data",
                                    "common_attrs": {"App": "MOD"},
                                    "single_attrs": [
                                        {"Name": "Type", "Value": "title", "function": "fixed"},
                                        {"Name": "Title_Sort_Name", "field": "Title", "maxlength": "22", "function": "field"},
                                        {"Name": "Title_Brief", "field": "Title", "maxlength": "19", "function": "field"},
                                        {"Name": "Title", "field": "Title", "maxlength": "54", "function": "field"},
                                        {"Name": "Episode_Name", "field": "Título em Português do Episódio", "maxlength": "128", "function": "field", "filter": "Season != ''"},
                                        {"Name": "Episode_ID", "function": "episode_id", "filter": "Season != ''"},
                                        {"Name": "Summary_Long", "target_language": "pt-BR", "maxlength": "1024", "field": "Summary_Medium", "function": "field_no_quotes"},
                                        {"Name": "Summary_Medium", "target_language": "pt-BR", "maxlength": "1024", "field": "Summary_Medium", "function": "field_no_quotes"},
                                        {"Name": "Summary_Short", "target_language": "pt-BR", "maxlength": "256", "field": "Summary_Medium", "function": "field_no_quotes"},
                                        {"Name": "Rating", "Options": "1,10,12,14,16,18,ER", "field": "Rating", "function": "field_validated"},
                                        {"Name": "Closed_Captioning", "Value": "N", "function": "fixed"},
                                        {"Name": "Run_Time", "field": "Run_Time", "function": "field", "type": "time_s"},
                                        {"Name": "Display_Run_Time","field": "Run_Time", "function": "field", "type": "time"},
                                        {"Name": "Year", "field": "Year", "function": "field"},
                                        {
                                            "Name": "Country_of_Origin",
                                            "field": "Country_of_Origin",
                                            "function": "field"
                                        },
                                        {"Name": "Actors_Display", "field": "Actors", "function": "field"},
                                        {"Name": "Actors", "field": "Actors", "function": "split", "function2": "surname_name"},
                                        {"Name": "Director", "field": "Director", "function": "surname_name"},
                                        {"Name": "Director_Display", "field": "Director", "function": "field"},
                                        {"Name": "Category", "field": "Content_Category", "function": "field"},
                                        {"Name": "Genre", "target_language": "pt-BR", "field": "Genre 1", "function": "field"},
                                        {"Name": "Billing_ID", "field": "Billing_ID", "function": "field"},
                                        {"Name": "Licensing_Window_Start", "field": "Licensing_Window_Start", "function": "field_date"},
                                        {"Name": "Licensing_Window_End", "field": "Licensing_Window_End", "function": "field_date"},
                                        {"Name": "Preview_Period", "Value": "0", "function": "fixed"},
                                        {"Name": "Provider_QA_Contact", "Value": "MediaCenter", "function": "fixed"},
                                        {"Name": "Contract_Name", "field": "Original_Title", "function": "field"}
                                    ]
                                }
                            ]
                        },
                        {
                            "Name": "Asset",
                            "elements": [
                                {
                                    "Name": "Metadata",
                                    "options": {"Name": "section", "Value": "movie"},
                                    "elements": [
                                        {
                                            "Name": "AMS",
                                            "attrs": [
                                                {"Name": "Provider", "field": "Provider", "function": "field"},
                                                {"Name": "Product", "Value": "", "function": "fixed"},
                                                {"Name": "Asset_Name", "maxlength": "42", "field": "Title", "function": "field_suffix", "suffix": "_Movie"},
                                                {"Name": "Version_Major", "Value": "1", "function": "fixed"},
                                                {"Name": "Version_Minor", "Value": "0", "function": "fixed"},
                                                {"Name": "Creation_Date", "function": "date"},
                                                {"Name": "Description", "field": "Title", "function": "field"},
                                                {"Name": "Provider_ID", "field": "Provider_ID", "function": "field"},
                                                {"Name": "Asset_ID", "function": "assetid", "suffix_number": 3, "prefix": "Provider"},
                                                {"Name": "Asset_Class", "Value": "movie", "function": "fixed"}
                                            ]
                                        },
                                        {
                                            "Name": "App_Data",
                                            "common_attrs": {"App": "MOD"},
                                            "single_attrs": [
                                                {"Name": "Type", "Value": "movie", "function": "fixed"},
                                                {"Name": "Watermarking", "Value": "N", "function": "fixed"},
                                                {"Name": "Audio_Type", "Options": "Mono,Dolby 5.1,Stereo", "field": "Audio_Type", "function": "field_validated"},
                                                {"Name": "Viewing_Can_Be_Resumed", "Value": "Y", "function": "fixed"},
                                                {"Name": "CGMS_A", "Value": "3", "function": "fixed"},
                                                {"Name": "Languages", "Value": "pt", "function": "fixed", "filter": "Dublado == 'pt'"},
                                                {
                                                    "Name": "Languages",
                                                    "field": "Language_movie",
                                                    "function": "split",
                                                    "function2": "field_validated",
                                                    "filter": "Dublado != 'pt'"
                                                }
                                            ]
                                        }
                                    ]
                                },
                                {"Name": "Content", "attrs": [{"Name": "Value", "field": "ID", "function": "field_suffix", "suffix": ".ts"}]}
                            ]
                        },
                        {
                            "Name": "Asset",
                            "elements": [
                                {
                                    "Name": "Metadata",
                                    "options": {"Name": "section", "Value": "poster"},
                                    "elements": [
                                        {
                                            "Name": "AMS",
                                            "attrs": [
                                                {"Name": "Provider", "field": "Provider", "function": "field"},
                                                {"Name": "Product", "Value": "", "function": "fixed"},
                                                {"Name": "Asset_Name", "maxlength": "42", "field": "Title", "function": "field_suffix", "suffix": "_Poster"},
                                                {"Name": "Version_Major", "Value": "1", "function": "fixed"},
                                                {"Name": "Version_Minor", "Value": "0", "function": "fixed"},
                                                {"Name": "Creation_Date", "function": "date"},
                                                {"Name": "Description", "field": "Title", "function": "field"},
                                                {"Name": "Provider_ID", "field": "Provider_ID", "function": "field"},
                                                {"Name": "Asset_ID", "function": "assetid", "suffix_number": 4, "prefix": "Provider"},
                                                {"Name": "Asset_Class", "Value": "poster", "function": "fixed"}
                                            ]
                                        },
                                        {
                                            "Name": "App_Data",
                                            "common_attrs": {"App": "MOD"},
                                            "single_attrs": [
                                                {"Name": "Type", "Value": "poster", "function": "fixed"},
                                                {"Name": "Content_FileSize", "field": "Poster Size", "function": "field"},
                                                {"Name": "Content_CheckSum", "field": "Poster MD5", "function": "field"}
                                            ]
                                        }
                                    ]
                                },
                                {"Name": "Content", "attrs": [{"Name": "Value", "field": "ID", "function": "field_suffix", "suffix": ".jpg"}]}
                            ]
                        },
                        {
                            "Name": "Asset",
                            "filter": "Trailer_ID != ''",
                            "elements": [
                                {
                                    "Name": "Metadata",
                                    "options": {"Name": "section", "Value": "preview"},
                                    "elements": [
                                        {
                                            "Name": "AMS",
                                            "attrs": [
                                                {"Name": "Provider", "field": "Provider", "function": "field"},
                                                {"Name": "Product", "Value": "", "function": "fixed"},
                                                {"Name": "Asset_Name", "Value": "", "maxlength": "42", "field": "Title", "function": "field_suffix", "suffix": "_Preview"},
                                                {"Name": "Version_Major", "Value": "1", "function": "fixed"},
                                                {"Name": "Version_Minor", "Value": "0", "function": "fixed"},
                                                {"Name": "Creation_Date", "Value": "", "field": "", "function": "date"},
                                                {"Name": "Description", "Value": "", "field": "Title", "function": "field"},
                                                {"Name": "Provider_ID", "field": "Provider_ID", "function": "field"},
                                                {"Name": "Asset_ID", "function": "assetid", "suffix_number": 5, "prefix": "Provider"},
                                                {"Name": "Asset_Class", "Value": "preview", "function": "fixed"}
                                            ]
                                        },
                                        {
                                            "Name": "App_Data",
                                            "common_attrs": {"App": "MOD"},
                                            "single_attrs": [
                                                {"Name": "Type", "Value": "preview", "function": "fixed"},
                                                {"Name": "Rating", "Options": "1,10,12,14,16,18,ER", "field": "Rating", "function": "field_validated"},
                                                {"Name": "Run_Time", "field": "Duração Trailer", "function": "field", "type": "time_s"},
                                                {"Name": "Audio_Type", "field": "Audio_Type", "function": "field"},
                                                {"Name": "Content_FileSize", "field": "Trailer Size", "function": "field"},
                                                {"Name": "Content_CheckSum", "field": "Trailer MD5", "function": "field"}
                                            ]
                                        }
                                    ]
                                },
                                {"Name": "Content", "attrs": [{"Name": "Value", "field": "ID", "function": "field_suffix", "suffix": "_trailer.ts"}]}
                            ]
                        }
                    ]
                }
            ]
        }
    ]
}
